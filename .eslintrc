{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": "off",
    "function-paren-newline": ["error", "consistent"]
  },
  "settings": {
    "react": { "pragma": "createElement" }
  },
  "overrides": [
    {
      "files": "**/*.test.js",
      "plugins": [ "jest" ],
      "env": {
        "jest/globals": true
      }
    }
  ]
}
